{
  "name": "electron-updater-example",
  "version": "0.1.0",
  "main": "main.js",
  "description": "electron-updater example project",
  "author": "kiki",
  "devDependencies": {
    "autoprefixer": "^8.4.1",
    "babel-loader": "^8.0.0-beta.2",
    "babel-polyfill": "^6.26.0",
    "babel/core": "^7.0.0-beta.46",
    "babel/preset-env": "^7.0.0-beta.46",
    "breakpoint-sass": "^2.7.1",
    "copy-webpack-plugin": "^4.5.1",
    "css-loader": "^0.28.11",
    "electron": "^1.8.4",
    "electron-builder": "^20.10.0",
    "html-webpack-plugin": "^3.2.0",
    "http-server": "^0.9.0",
    "image-webpack-loader": "^4.2.0",
    "libsass-compass-vertical-rhythm": "^1.0.0",
    "mini-css-extract-plugin": "^0.4.0",
    "node-sass": "^4.9.0",
    "open-browser-webpack-plugin": "0.0.5",
    "postcss-loader": "^2.1.4",
    "precss": "^3.1.2",
    "sass-loader": "^7.0.1",
    "style-loader": "^0.21.0",
    "susy": "^3.0.5",
    "url-loader": "^1.0.1",
    "vertical-rhythm": "^0.3.3",
    "webpack": "^4.6.0",
    "webpack-cli": "^2.1.2",
    "webpack-dev-server": "^3.1.4"
  },
  "dependencies": {
    "electron-log": "^2.2.14",
    "electron-updater": "^2.21.8",
    "jquery": "^3.3.1"
  },
  "scripts": {
    "electron-start": "electron .",
    "dist": "build --win --ia32",
    "pack": "build --win --ia32 --dir",
    "server": "http-server wwwroot/ -p 8080",
    "dev": "webpack && webpack-dev-server --open --progress --colors --useLocalIp"
  },
  "build": {
    "publish": [
      {
        "provider": "generic",
        "url": "http://127.0.0.1:8080/"
      }
    ],
    "appId": "electron-demo",
    "mac": {
      "category": "electron-demo",
      "icon": "./build/Mushroom.ico",
      "target": [
        "zip",
        "dmg"
      ]
    },
    "win": {
      "icon": "./build/Mushroom.ico",
      "target": [
        "nsis",
        "zip"
      ]
    },
    "linux": {
      "icon": "icons/Mushroom.ico"
    },
    "nsis": {
      "perMachine": true
    }
  }
}
